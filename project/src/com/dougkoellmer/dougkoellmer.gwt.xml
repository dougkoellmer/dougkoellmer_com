<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='r.app'>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.json.JSON'/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <inherits name="com.google.gwt.xml.XML"/>
  <inherits name="swarm.gwt_module"/>
  
  
  <!-- <set-property name="user.agent" value="ie9"/> -->
  
   <!-- <set-property name="user.agent" value="safari" />-->
  
  <set-property name="gwt.logging.logLevel" value="ALL"/>          	# To change the default logLevel
  <set-property name="gwt.logging.enabled" value="TRUE"/>            # To disable logging
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>  # To disable a default Handler
  <!-- <set-property name="gwt.logging.popupHandler" value="DISABLED" /> -->
  
  <!-- <set-property name="compiler.stackMode" value="emulated" /> -->
  
  <add-linker name="std"/>
  
  <!--<set-configuration-property name="devModeRedirectEnabled" value="true" />-->
  
  <!--<set-configuration-property name="installCode" value="true" />
  <set-property name="compiler.useSourceMaps" value="true" /> -->

  <!-- Specify the app entry point class.                         -->
   <entry-point class='com.dougkoellmer.client.app.ClientApp'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
